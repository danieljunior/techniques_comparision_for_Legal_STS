    "# add PyTextRank to the spaCy pipeline\n",
    "# doc = nlp(text)\n",
    "\n",
    "# # examine the top-ranked phrases in the document\n",
    "# for phrase in doc._.phrases:\n",
    "#     print(phrase.text)\n",
    "#     print(phrase.rank, phrase.count)\n",
    "#     print(phrase.chunks)"
