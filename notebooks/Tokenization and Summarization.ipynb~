    "resp = []\n",
    "last = \"\"\n",
    "for sent in nlp(text).sents:\n",
    "    t = (last + \" \" +sent.text).strip()\n",
    "    last = \"\"\n",
    "    \n",
    "    if 'RUBRICA' in t.upper() or 'FLS' in t.upper() \\\n",
    "        or re.search(r\"^\\n?\\d+(PROCESSO)\", t.upper()) \\\n",
    "        or t == '.' or t.isnumeric():\n",
    "        continue\n",
    "\n",
    "    tmp = t.replace('\\n',' ').strip()\n",
    "    if tmp.split(' ')[0].isnumeric():\n",
    "        t = ' '.join(tmp.split(' ')[1:])\n",
    "    \n",
    "    if not (len(re.findall(r\"\\(\",t)) == len(re.findall(r\"\\)\",t))) \\\n",
    "        or t.split(' ')[-1] in [\"n°\",\"TCE-\"] or re.search(r\"^\\d+\\.$\", t.upper()):\n",
    "        last = t\n",
    "    \n",
    "    if last == \"\":\n",
    "        resp.append(t.replace(\"\\n\",' '))\n",
    "        \n",
    "#     if \"\" in t:\n",
    "#         import pdb; pdb.set_trace()\n",
    "for p in resp:\n",
    "    print(\"#\", p)\n",
    "    "
