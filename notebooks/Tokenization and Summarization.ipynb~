    "from spacy.lang.tokenizer_exceptions import BASE_EXCEPTIONS\n",
    "from spacy.symbols import ORTH\n",
    "from spacy.util import update_exc\n",
    "\n",
    "\n",
    "_exc = {}\n",
    "\n",
    "\n",
    "for orth in [\n",
    "    \"Adm.\",\n",
    "    \"Art.\",\n",
    "    \"art.\",\n",
    "    \"Av.\",\n",
    "    \"av.\",\n",
    "    \"Cia.\",\n",
    "    \"dom.\",\n",
    "    \"Dr.\",\n",
    "    \"dr.\",\n",
    "    \"e.g.\",\n",
    "    \"E.g.\",\n",
    "    \"E.G.\",\n",
    "    \"e/ou\",\n",
    "    \"ed.\",\n",
    "    \"eng.\",\n",
    "    \"etc.\",\n",
    "    \"Fund.\",\n",
    "    \"Gen.\",\n",
    "    \"Gov.\",\n",
    "    \"i.e.\",\n",
    "    \"I.e.\",\n",
    "    \"I.E.\",\n",
    "    \"Inc.\",\n",
    "    \"Jr.\",\n",
    "    \"km/h\",\n",
    "    \"Ltd.\",\n",
    "    \"Mr.\",\n",
    "    \"no.\",\n",
    "    \"p.m.\",\n",
    "    \"Ph.D.\",\n",
    "    \"Rep.\",\n",
    "    \"Rev.\",\n",
    "    \"S/A\",\n",
    "    \"Sen.\",\n",
    "    \"Sr.\",\n",
    "    \"sr.\",\n",
    "    \"Sra.\",\n",
    "    \"sra.\",\n",
    "    \"vs.\",\n",
    "    \"tel.\",\n",
    "    \"pág.\",\n",
    "    \"pag.\",\n",
    "]:\n",
    "    _exc[orth] = [{ORTH: orth}]\n",
    "\n",
    "\n",
    "TOKENIZER_EXCEPTIONS = update_exc(BASE_EXCEPTIONS, _exc)\n",
    "TOKENIZER_EXCEPTIONS"
   ]
